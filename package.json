    "type": "git",
    "url": "git+https://gitlab.com/digitalia/ts-next-starter.git"
  },
  "bugs": {
    "url": "https://gitlab.com/digitalia/ts-next-starter/issues"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "test:coverage": "jest --coverage",
    "sb": "start-storybook -p 6006",
    "sb:build": "build-storybook",
    "sb:test": "cd .storybook && jest storyShot.test.js && cd .."
  },
  "husky": {
